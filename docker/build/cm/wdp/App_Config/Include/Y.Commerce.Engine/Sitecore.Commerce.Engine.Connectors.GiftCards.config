<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
  <sitecore role:require="Standalone or ContentDelivery or ContentManagement">
    <pipelines>
      <translate.giftCardToEntity>
        <processor type="Sitecore.Commerce.Engine.Connect.Pipelines.GiftCards.TranslateGiftCardToEntity, Sitecore.Commerce.Engine.Connect">
          <param ref="entityFactory" />
        </processor>
      </translate.giftCardToEntity>

      <commerce.giftCards.getGiftCard>
        <processor type="Sitecore.Commerce.Engine.Connect.Pipelines.GiftCards.GetGiftCard, Sitecore.Commerce.Engine.Connect" />
      </commerce.giftCards.getGiftCard>
    </pipelines>
  </sitecore>
</configuration>
